{
  "name": "<%-projectName%>",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    <%_ packages.forEach(function(package) { _%>
    "@brainhubeu/<%-package%>": "*",
    <%_ }) _%>
    <%_ if(packages.includes('hadron-express')) { _%>
    "express": "^4.16.3",
    "body-parser": "*",
    <%_ } _%>
    <%_ if(packages.includes('hadron-typeorm')) { _%>
    "mysql": "*",
    <%_ } _%>
    "@brainhubeu/hadron-core": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "*",
    "eslint": "*"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js"
  }
}
